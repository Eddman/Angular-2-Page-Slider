{
    "name": "@netocny/ng-page-slider",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/Eddman/ng-page-slider"
    },
    "scripts": {
        "ng": "ng",
        "build": "ng build && npm run themes && npm run static",
        "lint": "ng lint",
        "themes": "bundle-scss --config",
        "static": "cp LICENSE README.md .npmignore dist/netocny/ng-page-slider/ && cp .npmrc_publish dist/netocny/ng-page-slider/.npmrc"
    },
    "bundleStyle": {
        "dest": "./dist/netocny/ng-page-slider/ng-page-slider.scss",
        "mask": "./projects/netocny/ng-page-slider/src/styles.scss",
        "sort": [
            "component"
        ]
    },
    "dependencies": {
        "@angular/animations": "~8.2.14",
        "@angular/common": "~8.2.14",
        "@angular/compiler": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14",
        "rxjs": "~6.5.3",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.19",
        "@angular-devkit/build-ng-packagr": "~0.803.19",
        "@angular/cli": "~8.3.19",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@types/jasmine": "~3.4.6",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "~10.17.5",
        "bundle-scss": "^1.4.10",
        "codelyzer": "^5.2.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ng-packagr": "^5.7.1",
        "protractor": "~6.0.0",
        "tsickle": "0.37.0",
        "tslib": "^1.10.0",
        "tslint": "~5.20.1",
        "typescript": "~3.5.3"
    }
}
