{
    "name": "@netocny/ng-page-slider",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/Eddman/ng-page-slider"
    },
    "scripts": {
        "ng": "ng",
        "build": "ng build --prod && npm run themes && npm run static",
        "lint": "ng lint",
        "themes": "bundle-scss --config",
        "static": "cp LICENSE README.md .npmignore dist/netocny/ng-page-slider/ && cp .npmrc_publish dist/netocny/ng-page-slider/.npmrc"
    },
    "bundleStyle": {
        "dest": "./dist/netocny/ng-page-slider/ng-page-slider.scss",
        "mask": "./projects/netocny/ng-page-slider/src/styles.scss",
        "sort": [
            "component"
        ]
    },
    "dependencies": {
        "@angular/animations": "~9.0.5",
        "@angular/common": "~9.0.5",
        "@angular/compiler": "~9.0.5",
        "@angular/core": "~9.0.5",
        "@angular/forms": "~9.0.5",
        "@angular/platform-browser": "~9.0.5",
        "@angular/platform-browser-dynamic": "~9.0.5",
        "@angular/router": "~9.0.5",
        "rxjs": "~6.5.4",
        "tslib": "^1.11.1",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.5",
        "@angular-devkit/build-ng-packagr": "~0.900.5",
        "@angular/cli": "~9.0.5",
        "@angular/compiler-cli": "~9.0.5",
        "@angular/language-service": "~9.0.5",
        "@types/jasmine": "~3.5.8",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "^12.11.1",
        "bundle-scss": "^1.4.10",
        "codelyzer": "^5.2.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "~3.1.1",
        "karma-jasmine-html-reporter": "^1.5.2",
        "ng-packagr": "^9.0.2",
        "protractor": "~6.0.0",
        "tslint": "~5.20.1",
        "typescript": "~3.7.5"
    }
}
